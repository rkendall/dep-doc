{
  "default": {
    "workingDir": ".",
    "directoryFilter": [
      ".*",
      "node_modules",
      "build"
    ],
    "referencingFileFilter": [
      "*.js",
      "*.jsx"
    ],
    "referencedFileFilter": [
      "*.*"
    ],
    "searchPatterns": [
      "(^|\\s)import\\s+([\\w\\-\\{\\}\\,\\*\\$\\s]+\\s+)?(['\"]){{valuePattern}}\\3",
      "(^|[^\\w-])require *\\( *(['\"]){{valuePattern}}\\2 *\\)"
    ],
    "valuePattern": "\\.+/[^'\"\\s]+",
    "excludeText": [
      "// *[^\\n]+\\n",
      "/\\*[\\s\\S]+?\\*/"
    ]
  },
  "html": {
    "referencingFileFilter": [
      "*.html",
      "*.js"
    ],
    "referencedFileFilter": [
      "*.*"
    ],
    "searchPatterns": [
      "<link +[^>]*href=(['\"]){{valuePattern}}\\1[^>]*?/?>",
      "<a +[^>]*href=(['\"]){{valuePattern}}\\1[^>]*?/?>",
      "<script +[^>]*src=(['\"]){{valuePattern}}\\1[^>]*?/?>",
      "<img +[^>]*src=(['\"]){{valuePattern}}\\1[^>]*?/?>"
    ],
    "valuePattern": "[^'\"\\s:;#]+"
  }
}